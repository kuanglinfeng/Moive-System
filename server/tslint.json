{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-empty": true,
        "no-console": false,
        "interface-name":true,
        "ordered-imports": false,
        "arrow-parens": false,
        "semicolon": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "member-ordering": false
    },
    "rulesDirectory": []
}